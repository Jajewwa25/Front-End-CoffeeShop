    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Update Employee</title>
    <link rel="stylesheet" href="../css/updatecustomer.css">
  </head>
  <body>

    <h1>Update Customer</h1>
    <form action="/updateemployee/<%=Employee.employee_id%>" method="post">
      <label>Username : 
      <input type="text" name="username" id="username" value="<%= Employee.username%>" /></label>
      <br />
      <label>Password : 
      <input type="text" name="password" id="password" value="<%= Employee.password%>" /></label>
      <br />
      <label>Age : 
        <input type="text" name="age" id="age" value="<%= Employee.age%>" /></label>
        <br>
      <label>Position : 
        <input type="text" name="position"  id="position" value="<%= Employee.position%>" /></label>
        <br>
     <label>Address : 
        <input type="text" name="address"  id="address" value="<%= Employee.address%>" /></label>
        <br>
    <label>Tel : 
        <input type="text" name="tel"  id="tel" value="<%= Employee.tel%>" /></label>
        <br>
      <label>Email : 
        <input type="text" name="email" id="email" value="<%= Employee.email%>" /></label>
        <br />
      <center><input type="submit" value="Update" /></center>
    </form>

    <center><a href="/employee">Back to List</a></center>
